{
  "manifest_version": 2,
  "name": "Messenger Scraper",
  "description": "Scrape messages on messenger.com",
  "version": "0.1",
  "minimum_chrome_version": "10.0",
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "devtools_page": "devtools.html",
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}